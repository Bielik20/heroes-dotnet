{
  "JwtIssuerOptions": {
    "Issuer": "HeroesTokenServer",
    "Audience": "actual server address"
  },
  "Logging": {
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Warning"
    }
  },
  "ConnectionStrings": {
    "Current": "SqliteConnection",
    "SqliteConnection": "DataSource=heroes-dotnet.db",
    "DefaultConnection": "Server=(localdb)\\mssqllocaldb;Database=HeroesDotnet;Trusted_Connection=True;MultipleActiveResultSets=true"
  }
}
